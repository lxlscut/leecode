   "source": [
    "def generate(l,r,item,result):\n",
    "    if l > r :\n",
    "        return \n",
    "    if l == 0 and r == 0:\n",
    "        result.append(item)\n",
    "    if l>0 :\n",
    "        generate(l-1,r,item+\"(\",result)\n",
    "    if r>0:\n",
    "        generate(l,r-1,item+\")\",result)\n",
    "    \n",
    "result = []\n",
    "generate(3,3,\"\",result)\n",
    "print(result)"
   ]
